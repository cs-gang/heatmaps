{
  "name": "heatmaps",
  "version": "1.0.0",
  "description": "## Contributing",
  "main": "",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url ./heatmaps/static/css/main.sass ./heatmaps/static/css/main.css",
    "css": "npm run css-build -- --watch",
    "minimize-js": "npx terser -c -m -o ./heatmaps/templates/$npm_config_jsfilepre -- ./heatmaps/templates/$npm_config_jsfilepost",
    "parse-jsx": "npx babel ./heatmaps/templates/$npm_config_jsfilepre --out-file ./heatmaps/templates/$npm_config_jsfilepost --presets react-app/prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cs-gang/heatmaps.git"
  },
  "author": "cs-gang",
  "license": "",
  "bugs": {
    "url": "https://github.com/cs-gang/heatmaps/issues"
  },
  "homepage": "https://github.com/cs-gang/heatmaps#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "node-sass": "^5.0.0",
    "terser": "^5.6.1"
  }
}
